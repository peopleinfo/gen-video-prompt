{
  "name": "gen-video-prompt-mcp",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "MCP server for browsing local Sora 2 prompt docs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "gui": "npm run build && node dist/gui/server.js",
    "gui:llm": "ENABLE_COMMAND_LLM=1 npm run gui",
    "gui:http-llm": "ENABLE_HTTP_LLM=1 npm run gui",
    "gui:all-llm": "ENABLE_COMMAND_LLM=1 ENABLE_HTTP_LLM=1 npm run gui",
    "test:gui": "npm run build && node scripts/test-gui.mjs",
    "dev": "tsx src/server.ts",
    "rename:data": "tsx src/rename-data.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.1.0",
    "fast-glob": "^3.3.2",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
