{
  "name": "gen-video-prompt-mcp",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "MCP server for browsing local Sora 2 prompt docs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "gui": "npm run build && node dist/gui/server.js",
    "gui:llm": "ENABLE_COMMAND_LLM=1 npm run gui",
    "gui:http-llm": "ENABLE_HTTP_LLM=1 npm run gui",
    "gui:all-llm": "ENABLE_COMMAND_LLM=1 ENABLE_HTTP_LLM=1 npm run gui",
    "ui": "HTTPS=1 SSL_KEY=tmp/certs/gui.key.pem SSL_CERT=tmp/certs/gui.cert.pem pnpm gui:all-llm",
    "gui:restart": "node scripts/restart-gui.mjs",
    "gui:cert": "mkdir -p tmp/certs && openssl req -x509 -newkey rsa:2048 -nodes -keyout tmp/certs/gui.key.pem -out tmp/certs/gui.cert.pem -days 365 -subj \"/CN=localhost\"",
    "gui:watch": "MCP_SERVER_DEV=1 tsx watch src/gui/server.ts",
    "gui:watch:llm": "ENABLE_COMMAND_LLM=1 MCP_SERVER_DEV=1 tsx watch src/gui/server.ts",
    "gui:watch:all-llm": "ENABLE_COMMAND_LLM=1 ENABLE_HTTP_LLM=1 MCP_SERVER_DEV=1 tsx watch src/gui/server.ts",
    "mcp:watch": "tsx watch src/server.ts",
    "test:gui": "npm run build && node scripts/test-gui.mjs",
    "test:gui:codex-image": "node scripts/test-gui-codex-image.mjs",
    "test:gui:gemini-chat": "npm run build && node scripts/test-gui-gemini-chat.mjs",
    "dev": "tsx src/server.ts",
    "rename:data": "tsx src/rename-data.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.1.0",
    "fast-glob": "^3.3.2",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
